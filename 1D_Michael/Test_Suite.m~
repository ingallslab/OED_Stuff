numFits = 10;
numExp = 10;
numTrials = 200;
options = struct('DataSource','Norm_Linspace_LNA','SystemSize',90);
fits = generateFits(numFits,numExp,numTrials,options);

disp(fits);

std_a0 = std(fits(:,1));
std_a = std(fits(:,2));
std_K = std(fits(:,3));
std_n = std(fits(:,4));
a_a0 = mean(fits(:,1));
a_a = mean(fits(:,2));
a_K = mean(fits(:,3));
a_n = mean(fits(:,4));


disp('Covariance Matrix');
disp(cov(fits));
disp(strcat("Variance in a0: \t',num2str((std_a0)^2)));
disp(strcat("Variance in a: 't',num2str((std_a)^2)));
disp(strcat("Variance in K: t',num2str((std_K)^2)));
disp(strcat("Variance in n: 't',num2str((std_n)^2)));

disp(strcat("Mean a0: 't",num2str(a_a0)));
disp(strcat("Mean a: 't",num2str(a_a)));
disp(strcat("Mean K: 't",num2str(a_K)));
disp(strcat("Mean n: 't",num2str(a_n)));